<?xml version="1.0" encoding="UTF-8"?>
<Environment>
	<Gather class="com.briup.client.impl.GatherImpl">
		<!--读取文件的路径 -->
		<path>src/main/java/radwtmp</path>
	</Gather>

	<Client class="com.briup.client.impl.ClientImpl">
		<ip>127.0.0.1</ip>
		<port>10001</port>
		<backup>src/main/java/backup</backup>
	</Client>

	<Server class="com.briup.server.impl.ServerImpl">
		<port>10001</port>
		<backup>src/main/java/backup</backup>
	</Server>

	<DBStore class="com.briup.server.impl.DBStoreImpl">
		<batchSize>1000</batchSize>
	</DBStore>

	<Log class="com.briup.util.impl.LogImpl">
		<path>
			src/main/java/log4j.properties
		</path>
	</Log>
	<Connection class="com.briup.util.ConnectionFactory">
		<path>
			src/main/java/jdbc.properties
		</path>
	</Connection>
</Environment>
